<h1>Workers#index</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Apellido</th>
      <th>Profesion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.name %></td>
        <td><%= user.last_name %></td>
        <td><%= user.profesion.name %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Volver al inicio', root_path %>